@model ActionResultTypes.Models.Address

<h3>Employee Address</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BlockNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.TextBoxFor(model => model.BlockNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StreetName)
        </dt>
        <dd class="col-sm-10">
            @Html.TextBoxFor(model => model.StreetName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.City)
        </dt>
        <dd class="col-sm-10">
            @Html.TextBoxFor(model => model.City)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.State)
        </dt>
        <dd class="col-sm-10">
            @Html.TextBoxFor(model => model.State)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Country)
        </dt>
        <dd class="col-sm-10">
            @Html.TextBoxFor(model => model.Country)
        </dd>
    </dl>
</div>
